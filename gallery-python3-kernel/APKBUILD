pkgname=gallery-ptyhon3-kernel
pkgver=0.1.0
pkgrel=0
pkgdesc="Python 3 kernel for the Jupyter Gallery alpine client"
url="https://github.com/jupyter-gallery/alpine-testing"
arch="x86_64"
license="MIT"
depends="python3 python3-dev openssl-dev libffi-dev py3-openssl py3-cryptography py3-six py3-cparser py3-cffi"
depends_dev=""
makedepends="$depends_dev"
install=""
subpackages=""
source="https://pypi.python.org/packages/f8/ad/0e77a853c745a15981ab51fa9a0cb4eca7a7a007b4c1970106ee6ba01e0c/entrypoints-0.2.2-py2.py3-none-any.whl"

build() {
	return 0;
}

package() {
	cd "$srcdir"
	pip3 install --root="$pkgdir" entrypoints-0.2.2-py2.py3-none-any.whl
	pip3 install --root="$pkgdir" ipykernel ipywidgets 
	pip3 install --root="$pkgdir" http://github.com/jupyter-gallery/pypki2/tarball/master#egg=package-1.0
	pip3 install --root="$pkgdir" --no-deps http://github.com/jupyter-gallery/ipydeps/tarball/master#egg=package-1.0
}

