sha256sums="1ccd374e6a85414837ebb934ce9504ac4153576e59f7efbf802a393b5bd8997b  pandas-0.19.1.tar.gz"
sha512sums="870f896f8b7f9c7ab3356791b6bf3aa4c4bbcc801dbd8a15e09666ffa91def06f3ab0f80dadf361e945ae39b4ce563c9562d4a9768b82d7ecb59e92fd66f153e  pandas-0.19.1.tar.gz"
