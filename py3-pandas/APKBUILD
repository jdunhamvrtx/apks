# Maintainer: Jupyter Gallery Team <team@jupyter.gallery>
pkgname=py3-pandas
_pkgname=pandas
pkgver=0.18.1
pkgrel=0
pkgdesc="Cross-section and time series data analysis toolkit"
url="http://pandas.pydata.org"
arch="all"
license="BSD"
depends="python3"
depends_dev=""
makedepends="python3-dev py3-dateutil py3-numpy-dev py3-tz cython3-dev"
install=""
subpackages=""
source="$_pkgname-$pkgver.tar.gz::https://github.com/pydata/$_pkgname/archive/v$pkgver.tar.gz"

builddir="${srcdir}/${_pkgname}-$pkgver"
build() {
	cd "$builddir" || return 1
	python3 setup.py build || return 1
}

package() {
	cd "$builddir"
	python3 setup.py install --prefix=/usr --root="$pkgdir" || return 1
}

